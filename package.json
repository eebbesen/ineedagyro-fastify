{
  "name": "ineedagyro-fastify",
  "version": "1.0.0",
  "description": "Find your nearest gyros",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "coverage": "nyc npm test",
    "dev": "fastify start -w -l info -P app.js",
    "lint": "biome lint .",
    "lint_fix": "biome lint --write ./**/*.js",
    "start": "fastify start -l info app.js",
    "test": "node --test test/**/*.test.js"
  },
  "keywords": [
    "gyros",
    "gyro"
  ],
  "author": "eebbesen",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/sensible": "^6.0.0",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "nyc": "^17.1.0"
  }
}
